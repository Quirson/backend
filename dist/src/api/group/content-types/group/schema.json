            "allowedTypes": [
                "images",
                "files",
                "videos",
                "audios"
            ]
        },
        "isPrivate": {
            "type": "boolean"
        },
        "memberCount": {
            "type": "biginteger"
        },
        "category": {
            "type": "string"
        },
        "rules": {
            "type": "text"
        },
        "group_members": {
            "type": "relation",
            "relation": "manyToMany",
            "target": "api::profile.profile",
            "inversedBy": "groups"
        },
        "admin": {
            "type": "relation",
            "relation": "oneToOne",
            "target": "api::profile.profile"
        }
